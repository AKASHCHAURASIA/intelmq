<script>
	var validateDispName = function(display_name){
		var display_name_filter_1=/[<>=\\"''%;()#*\\/\?&+]/;
        var display_name_filter_2=/^\S.*\S$/;
        var display_name_filter_3=/^(?!.*\s{2}).+$/;
        var errMsg="";
        var hasError = false;        
        if ($.trim(display_name) == "") {
            errMsg = 'Display Name cannot be blank';
        }
        else if ($.trim(display_name).length > 31 || $.trim(display_name).length < 6) {
            errMsg = 'Display Name must be between 6 and 31 characters.  Please try again';
        } 
        else if (display_name_filter_1.test(display_name)) {
            errMsg = 'Invalid characters. The following characters cannot be used: [&lt;&gt;=\&quot;\&#39;%()#*\/?&amp;+];';
        }
        else if (!display_name_filter_2.test(display_name)) {
            errMsg = 'The Display Name you have chosen contains leading or trailing spaces. Please try again.';
        }
        else if (!display_name_filter_3.test(display_name)){
            errMsg = 'The Display Name you have chosen contains more than one consecutive space. Please try again.';
        }
        if (!$('#acceptTc').is(':checked')){
			errMsg = 'Must accept the Symantec Terms of Service';
		}
        return errMsg;
	}


	var dispErrMap = function(txt){
		if(txt == 'logErr')
			return "We&#39;re sorry, there was an error while processing your signin. Please try to signin again.";
		else if(txt == 'dispExist')
			return "The display name you have chosen is not available."+ " "  + " Please try again. ";
		else
			return txt+ " " + " Please try again. ";
	}
	
	var showMsg = function(){
			return false;
	}
	
	function showAlert() {
    	$("#message").addClass("in");
    	$("#message").show();
	}

	function closeAlert() {
    	$("#message").removeClass("in");
    	$("#message").hide();
	}
</script>

